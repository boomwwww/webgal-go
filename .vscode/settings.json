{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json,npm*,.npm*,pnpm*,.pnpm*,.editorconfig",
    "README.md": "*.md,,LICENSE",
    ".gitignore": ".git*",
    "prettier.config.js": "prettier*,.prettier*",
    "eslint.config.js": "eslint*,.eslint*",
    "tsconfig.json": "tsconfig.*.json,env.d.ts",
    "tsdown.config.ts": "*config.js,*config.ts,*config.json,*rc",
    "vite.config.ts": "*config.js,*config.json,*rc,.eslint*,eslint*,.prettier*,prettier*",
    "index.js": "index*"
  },
  "oxc.enable": true
}
