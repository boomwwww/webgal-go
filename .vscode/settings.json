{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json,npm*,.npm*,yarn*,.yarn*,pnpm*,.pnpm*,.editorconfig,LICENSE*,prettier.config.js",
    "README.md": "*.md",
    ".gitignore": ".git*",
    "tsconfig.json": "tsconfig.*.json,env.d.ts",
    "tsdown.config.ts": "*config.js,*config.ts,*config.json,*rc",
    "vite.config.ts": "*config.js,*config.json,*rc,.eslint*,eslint*,.prettier*,prettier*",
    "index.js": "index*"
  },
  "oxc.enable": true
}
